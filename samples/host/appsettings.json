{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Azure.Messaging.ServiceBus": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AzureServiceBus": {
        "ConnectionString": "Endpoint=sb://127.0.0.1:5673;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=SAS_KEY_VALUE;UseDevelopmentEmulator=true;",
        "Queues": [
            {
                "QueueName": "queue.1",
                "MaxCompetingConsumers": 4,
                "ConsumerType": "Edgamat.Messaging.Samples.Host.MyMessageConsumer, Edgamat.Messaging.Samples.Host",
                "Enabled": true
            },
            {
                "QueueName": "queue.1.dlq",
                "MaxCompetingConsumers": 4,
                "ConsumerType": "Edgamat.Messaging.Samples.Host.MyMessageDeadLetterConsumer, Edgamat.Messaging.Samples.Host",
                "Enabled": true
            },
            {
                "QueueName": "subscription.1.dlq",
                "MaxCompetingConsumers": 4,
                "ConsumerType": "Edgamat.Messaging.Samples.Host.MySubscriptionDeadLetterConsumer, Edgamat.Messaging.Samples.Host",
                "Enabled": true
            }
        ],
        "Subscriptions": [
            {
                "TopicName": "topic.1",
                "SubscriptionName": "subscription.3",
                "MaxCompetingConsumers": 4,
                "ConsumerType": "Edgamat.Messaging.Samples.Host.MySubscriptionMessageConsumer, Edgamat.Messaging.Samples.Host",
                "Enabled": true
            }
        ]
    }
}
